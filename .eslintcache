rror.message)\r\n    }\r\n  }\r\n  return (\r\n    <div className=\"register\">\r\n      <form>\r\n        <h1 className=\"title\">Register</h1>\r\n        <div className=\"error-register\">\r\n          <span style={{ display: invalidMessage ? '' : 'none' }}>\r\n            {invalidMessage} <i className=\"fas fa-times\"></i>\r\n          </span>\r\n        </div>\r\n        <Input\r\n          id=\"email\"\r\n          placeholder=\"Email\"\r\n          name=\"email\"\r\n          onChange={handleChangeText}\r\n          value={email}\r\n        />\r\n        <Gap height={20} />\r\n        <Input\r\n          id=\"password\"\r\n          placeholder=\"Password\"\r\n          type=\"password\"\r\n          name=\"password\"\r\n          value={password}\r\n          onChange={handleChangeText}\r\n        />\r\n        <Gap height={20} />\r\n        <Input\r\n          id=\"fullName\"\r\n          placeholder=\"Full Name\"\r\n          name=\"fullName\"\r\n          value={fullName}\r\n          onChange={handleChangeText}\r\n        />\r\n        <Gap height={30} />\r\n        <Button\r\n          title=\"Sign Up\"\r\n          width=\"100%\"\r\n          fontColor=\"#fff\"\r\n          padding=\"10px 0\"\r\n          fontSize=\"20px\"\r\n          onClick={handleRegister}\r\n        />\r\n        <Gap height={10} />\r\n        <p style={{ textAlign: 'left' }}>\r\n          Already have an account? Click{' '}\r\n          <strong onClick={props.onClickLogin} style={{ cursor: 'pointer' }}>\r\n            Here\r\n          </strong>{' '}\r\n        </p>\r\n      </form>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default FormRegister\r\n","H:\\2. WEB DEV\\DUMBWAYS BATCH19\\waysgallery\\client\\src\\components\\atoms\\Input\\index.js",[],"H:\\2. WEB DEV\\DUMBWAYS BATCH19\\waysgallery\\client\\src\\context\\appContext.js",[],"H:\\2. WEB DEV\\DUMBWAYS BATCH19\\waysgallery\\client\\src\\config\\api.js",[],"H:\\2. WEB DEV\\DUMBWAYS BATCH19\\waysgallery\\client\\src\\pages\\Hom